<?xml version="1.0" ?>
<config
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
	<system>
		<tab id="advanced" translate="label" sortOrder="999">
			<label>Advanced</label>
		</tab>
		<section id="speed_up" sortOrder="10"
                 showInWebsite="1" showInStore="0" showInDefault="0" translate="label">
			<label>Speed up</label>
			<tab>advanced</tab>
			<resource>HenriqueKieckbusch_SpeedupPages::config_henriquekieckbusch_speeduppages</resource>
			<group id="speedup_module" sortOrder="10" showInWebsite="1"
                   showInStore="0" showInDefault="0" translate="label">
				<label>Speed up module</label>
				<field id="enabled" type="select" sortOrder="10"
                       showInWebsite="1" showInStore="0" showInDefault="0" translate="label">
					<label>Enable the module</label>
					<comment/>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
				</field>
				<field id="preload_products_pdp" type="select" sortOrder="10"
                       showInWebsite="1" showInStore="0" showInDefault="0" translate="label">
					<label>Preload the products inside product list pages and search?</label>
					<comment/>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
				</field>
				<field id="preload_category_pages" type="select" sortOrder="10"
                       showInWebsite="1" showInStore="0" showInDefault="0" translate="label">
					<label>Pre load category pages</label>
					<comment/>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
				</field>
				<field id="preload_cms_pages" type="select" sortOrder="10"
                       showInWebsite="1" showInStore="0" showInDefault="0" translate="label">
					<label>Pre load CMS pages</label>
					<comment/>
					<source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
				</field>
				<field id="preload_cart_after_add_to_the_cart" type="select" sortOrder="10"
                       showInWebsite="1" showInStore="0" showInDefault="0" translate="label">
					<label>Preload the cart page, after add to the cart</label>
					<comment/>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
				</field>
			</group>
		</section>
	</system>
</config>
